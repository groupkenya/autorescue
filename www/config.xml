<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns       = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "io.autorescue.www"
        versionCode = "1"
        version     = "1.0.0" >
<name>AutoRescue</name>
<description></description>
<author href="https://www.groupkenya.com" email="info@groupkenya.com">Group Kenya</author>
<gap:platform name="android"/>
<preference name="android-build-tool" value="gradle"/>
<preference name="permissions" value="none"/>
<preference name="disallowOverscroll" value="true"/>
<preference name="webviewbounce" value="false"/>
<preference name="loadUrlTimeoutValue" value="700000"/>
<preference name="android-targetSdkVersion" value="26"/>
<!-- PhoneGap plugin declaration -->
<plugin name="cordova-plugin-whitelist" source="npm" />
<plugin name="cordova-plugin-customurlscheme" spec="4.2.0">
<param name="URL_SCHEME" value="autorescue" /></plugin>
<gap:plugin name="cordova-plugin-x-socialsharing" spec="5.1.8" source="npm"/>
<plugin name="phonegap-plugin-push" spec="~1.9.0" source="npm">
<param name="SENDER_ID" value="679903762037"/></plugin>
<!-- Whitelist policy  -->
<access origin="*"/>
<allow-intent href="*"/>
<allow-navigation href="*"/>
<!-- Icons declaration -->
<icon src="img/icon.png" gap:platform="android"/>
</widget>
